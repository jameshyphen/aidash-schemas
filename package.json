{
    "name": "@aidash/graphql-schemas",
    "version": "1.3.0",
    "private": false,
    "description": "Canonical GraphQL schema & generated TS hooks for Aidash",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "graphql-codegen --config codegen.yml",
        "lint": "graphql-schema-linter \"schema/**/*.graphql\"",
        "prepublishOnly": "npm run build"
    },
    "dependencies": {
        "graphql": "^16.8.1"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^3.2.2",
        "@graphql-codegen/typescript": "^3.2.2",
        "@graphql-codegen/typescript-operations": "^3.2.2",
        "@graphql-codegen/typescript-react-apollo": "^3.2.2",
        "graphql-schema-linter": "^0.3.0"
    }
}